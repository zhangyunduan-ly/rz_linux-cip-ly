		scie7: serial@90031c00 {
			compatible = "renesas,r9a09g087-sci",
				     "renesas,rz-rsci";
			reg = <0 0x90031c00 0 0x400>;
			interrupt-names = "eri", "rxi", "txi", "tei";
			clocks = <&cpg CPG_MOD R9A09G077_SCIE7_CLK>;
			clock-names = "fck";
			power-domains = <&cpg>;
			status = "disabled";
		};

		scie8: serial@90130000 {
			compatible = "renesas,r9a09g087-sci",
				     "renesas,rz-rsci";
			reg = <0 0x90130000 0 0x400>;
			interrupt-names = "eri", "rxi", "txi", "tei";
