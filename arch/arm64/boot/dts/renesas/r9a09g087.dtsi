			clock-names = "fck";
			power-domains = <&cpg>;
			status = "disabled";
		};

		scie0: serial@90030000 {
			compatible = "renesas,r9a09g087-sci",
				     "renesas,rz-rsci";
			reg = <0 0x90030000 0 0x400>;
			interrupt-names = "eri", "rxi", "txi", "tei";
			clocks = <&cpg CPG_MOD R9A09G077_SCIE0_CLK>;
			clock-names = "fck";
			power-domains = <&cpg>;
			status = "disabled";
		};

		scie1: serial@90030400 {
			compatible = "renesas,r9a09g087-sci",
				     "renesas,rz-rsci";
			reg = <0 0x90030400 0 0x400>;
			interrupt-names = "eri", "rxi", "txi", "tei";
			clocks = <&cpg CPG_MOD R9A09G077_SCIE1_CLK>;
			clock-names = "fck";
			power-domains = <&cpg>;
			status = "disabled";
