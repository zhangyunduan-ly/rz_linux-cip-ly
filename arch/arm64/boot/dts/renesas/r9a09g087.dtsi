			clock-names = "fck";
			power-domains = <&cpg>;
			status = "disabled";
		};

		scie4: serial@90031000 {
			compatible = "renesas,r9a09g087-sci",
				     "renesas,rz-rsci";
			reg = <0 0x90031000 0 0x400>;
			interrupt-names = "eri", "rxi", "txi", "tei";
			clocks = <&cpg CPG_MOD R9A09G077_SCIE4_CLK>;
			clock-names = "fck";
			power-domains = <&cpg>;
			status = "disabled";
		};

		scie5: serial@90031400 {
			compatible = "renesas,r9a09g087-sci",
