			interrupt-names = "macirq", "eth_wake_irq", "eth_lpi";
			clocks =  <&cpg CPG_MOD 195>,
				  <&cpg CPG_MOD 194>,
				  <&cpg CPG_MOD 190>,
				  <&cpg CPG_MOD 192>,
				  <&cpg CPG_MOD 191>,
				  <&cpg CPG_MOD 193>,
				  <&ptp_clock>;
			clock-names = "master_bus", "slave_bus", "tx",
				      "tx_180", "rx", "rx_180", "ptp_ref";
			resets = <&cpg 177>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
			snps,write-requests = <15>;
			snps,read-requests = <15>;
			snps,burst-map = <0x7>;
