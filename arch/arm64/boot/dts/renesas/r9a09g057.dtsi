	thermal-zones {
		emergency {
			polling-delay = <1000>;
			on-temperature = <110000>;
			off-temperature = <95000>;
			target_cpus =   <&cpu1 &cpu2 &cpu3>;
			status = "disabled";
		};

		sensor_thermal0: sensor-thermal0 {
			polling-delay-passive = <250>;
			polling-delay = <1000>;
			thermal-sensors = <&tsu0 0>;

			trips {
				sensor0_crit: sensor0-crit {
					temperature = <120000>;
					hysteresis = <1000>;
					type = "critical";
				};

			};
		};

		sensor_thermal1: sensor-thermal1 {
			polling-delay-passive = <250>;
			polling-delay = <1000>;
